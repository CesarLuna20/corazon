{
  "expo": {
    "name": "bibix-game",
    "slug": "bibix-game",
    "scheme": "bibix-game",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "statusBar": {
      "hidden": true
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "plugins": [
      "expo-dev-client",
      "expo-font"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.bibix.game"
    }
  }
}
